{
  "name": "@design-system/themes",
  "version": "0.0.0",
  "description": "Theme configurations for the design system",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "echo 'lint:themes'",
    "test": "echo 'test:themes'",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@design-system/tokens": "workspace:*"
  },
  "devDependencies": {},
  "publishConfig": {
    "access": "restricted"
  }
}